<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>createWalletController</servlet-name>
        <servlet-class>com.dev.xrpwebtools.Controller.createWalletController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>createWalletController</servlet-name>
        <url-pattern>/process.walletcreation</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>com.dev.xrpwebtools.Controller.dashboardController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/process.dashboard</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>sendXRPController</servlet-name>
        <servlet-class>com.dev.xrpwebtools.Controller.sendXRPController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sendXRPController</servlet-name>
        <url-pattern>/process.sendxrp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>createNFTWalletController</servlet-name>
        <servlet-class>com.dev.xrpwebtools.Controller.createNFTWalletController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>createNFTWalletController</servlet-name>
        <url-pattern>/process.nftcreation</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>org.quartz.ee.servlet.QuartzInitializerListener</listener-class>
    </listener>
</web-app>